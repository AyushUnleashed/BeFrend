<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Mitram" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/themeApp</item>
        <item name="colorPrimaryVariant">@color/themeSecondaryApp</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/themeApp</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#DCDCDC</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="GeneralFABStyle1" parent="ShapeAppearance.Material3.SmallComponent">
    </style>
    <style name="TagChipStyle1" parent="Widget.MaterialComponents.Chip.Filter">
        <item name="chipStrokeColor">@color/themeApp</item>
        <item name="chipBackgroundColor">@color/themeSecondaryApp</item>
        <item name="android:textColor">@color/newBlack</item>
        <item name="android:strokeWidth">2.0</item>
    </style>

    <style name="TagChipStyle2" parent="Widget.MaterialComponents.Chip.Filter">
        <item name="chipBackgroundColor">@color/themeApp</item>
        <item name="android:textColor">@color/white</item>
    </style>

</resources>